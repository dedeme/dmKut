/// Change password page

import "dm/cgi";

///
process = \Rq -> {
  rq = js.rs(Rq.rq);
  switch (rq) {
    "changePass": return cgi.changePass(
      js.rs(Rq.user),
      js.rs(Rq.old),
      js.rs(Rq.new)
    );
    default: sys.fail("Value of rq (" + rq + ") is not valid");
  };
};
