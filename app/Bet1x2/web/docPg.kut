/// Home page

import "libdm/cgi";
import "libdm/log";
import "db";
import "strategies";

///
process = \Rq -> {
  rq = js.rs(Rq.rq);
  switch (rq) {
    "idata": {
      doc = {};
      for (s = strategies.list) dic.put(doc, s.id, js.ws(s.doc));
      return cgi.rp({
        years: js.wa(arr.map(db.years(), \y -> return js.ws(y);)),
        doc: js.wo(doc)
      });
    }
    default: sys.throw("Value of rq (" + rq + ") is not valid");
  }
};
