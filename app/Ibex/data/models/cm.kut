/// Buy and keep model.

import "data/mdSt";
import "data/model";

doc = """
  Some doc
"""
;

// \[f...] -> <mdSt>
init = \Closes -> return mdSt.new(true, 0.0, "");;

// \<mdSt>, f -> <mdSt>
next = \State, close ->
  return mdSt.new(false, close, State.Internal ? "b" : "");;

/// Constructor.
/// \-> <model>
new = \-> return model.new(
  "CM",
  "Comprar y mantener",
  doc,
  [],
  init,
  next
);;



