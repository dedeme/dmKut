/// Model results.

import "data/nkRs";

/// Creates a new Rs of type array.
///   model: Model identifier.
///   nkResults: Nick resuls.
/// \s, [<nkRs>...] -> <rs>
new = \model, NkResults ->
  return [model, NkResults];;

/// \[*(JS)...] -> <rs>
fromJs = \A -> return new(A[0], arr.map(A[1], nkRs.fromJs));;

