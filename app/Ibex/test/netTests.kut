import "update/net";

run = \ -> {
  // To initialize qibex
  // db.qibex().write(net.getIbex());

  Qs = net.getIbex();
  ValSum = [0.0];
  DatesOk = [true];
  PrevDate = ["000000"];
  for (DtVal = Qs) {
    d = DtVal[0];
    if (arr.empty(time.fromStrOp(d))) {
      sys.println("Bad date: " + d);
      DatesOk! = false;
    }
    if (d <= PrevDate!) {
      sys.println("Unsorted date: " + d);
      DatesOk! = false;
    }
    PrevDate! = d;

    ValSum! += DtVal[1];
  }
  assert DatesOk!;
  assert ValSum! > 0.0;

  sys.println("net tests finished");
};
