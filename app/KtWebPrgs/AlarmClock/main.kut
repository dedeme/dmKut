/// Main script.

import "KtWeb/rp";
import "db";
import "pgs/home";
import "showAlarm";

Args = sys.args();
ck = Args[0];

db.init();
if (ck == "alarm!") {
  showAlarm.run(Args[1]);
  return;
}

Rq = js.r(Args[1]);
rp.init(ck);

switch(Rq.source) {
  "Home": home.process(Rq);
  default: sys.throw("Value of source (" + Rq.source + ") is not valid");
}


