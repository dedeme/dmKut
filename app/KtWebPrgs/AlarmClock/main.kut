/// Main script.

import "KtWeb/rp";
import "db";
import "pgs/home";
import "showAlarm";

Args = sys.args();
ck = Args[0];

db.init();

if (ck == "alarm!") {
  showAlarm.run(Args[1]);
  return;
}

Rq = js.ro(Args[1]);
rp.init(ck);

source = js.rs(Rq.source);
switch(source) {
  "Home": home.process(Rq);
  default: sys.fail("Value of source (" + source + ") is not valid");
}


