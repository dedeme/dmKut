/// [<bkEntry>.] table.

import "cts";
import "data/bkEntry";
import "db";

fpath = path.cat([cts.dataPath, "bkEntry.tb"]);

/// Initializes table.
/// \ -> ()
init = \ -> if (!file.exists(fpath)) write([]);;

/// Reads table.
/// \ -> [<bkEntry>.]
read = \ -> return js.r(file.read(fpath));;

/// Writes table.
/// \[<bkEntry>.] -> ()
write = \tb -> file.write(fpath, js.w(tb));;

