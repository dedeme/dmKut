/// <fromServer> table.

import "cts";
import "data/fromServer";
import "db";

fpath = file.cat([cts.dataPath, "fromServer.tb"]);

/// Initializes table.
/// \ -> ()
init = \ -> if (!file.exists(fpath)) write(fromServer.newDefault());;

/// Reads table.
/// \ -> <fromServer>
read = \ -> return fromServer.fromJs(js.r(file.read(fpath)));;

/// Writes table.
/// \<fromServer> -> ()
write = \o -> file.write(fpath, js.w(o));;
