/// Package index page.

import "KtWeb/rp";
import "readers/ixReader";
import "data/indexTree";

/// Process request.
/// \m -> s
process = \Rq -> {
  switch (Rq.rq) {
    "index": {
      ixTOp = ixReader.read(Rq.pack);
      return rp.mk({
        indexOp: !ixTOp
          ? []
          : [ixTOp!]
      });
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};


