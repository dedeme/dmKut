/// Module page.

import "KtWeb/rp";
import "readers/mdReader";
import "data/doc";

/// Process request.
/// \m -> s
process = \Rq -> {
  switch (Rq.rq) {
    "doc": {
      docOp = mdReader.read(Rq.pack, Rq.path);
      return rp.mk({
        docOp
      });
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};


