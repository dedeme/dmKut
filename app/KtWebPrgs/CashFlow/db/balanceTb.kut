/// Balance table.

/// Create year balance table.
/// \s -> ()
mk = \ypath -> writeJs(ypath, js.w(0.0));;

/// \s -> s
readJs = \ypath -> return file.read(path.cat([ypath, "balance.tb"]));;

/// \s, s -> ()
writeJs = \ypath, jsStr -> file.write(path.cat([ypath, "balance.tb"]), jsStr);;
