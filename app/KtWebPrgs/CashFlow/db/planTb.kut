/// Plan table

/// Create year plan table.
/// \s -> ()
mk = \ypath -> writeJs(ypath, js.w([]));;

/// \s -> s
readJs = \ypath -> return file.read(path.cat([ypath, "plan.tb"]));;

/// \s, s -> ()
writeJs = \ypath, jsStr -> file.write(path.cat([ypath, "plan.tb"]), jsStr);;
