/// Budget editor page.

import "KtWeb/rp";
import "db/yearsDb";
import "db/diaryTb";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "updateDiary": {
      year = Rq.year;
      ypath = yearsDb.yearPath(year);
      diaryTb.writeJs(ypath, js.w(Rq.diary));
      sys.print(rp.mkEmpty());
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};
