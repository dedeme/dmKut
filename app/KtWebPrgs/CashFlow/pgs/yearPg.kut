/// Year page.

import "KtWeb/rp";
import "db/yearsDb";

/// \{*.} -> s
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      return rp.mk({
          Years: yearsDb.list()
        });
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};
