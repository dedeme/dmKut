/// Pan page.

import "KtWeb/rp";
import "db/yearsDb";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      sys.print(rp.mk({
        years: yearsDb.list()
      }));
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};

