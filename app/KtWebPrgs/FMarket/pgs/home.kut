/// Home page.

import "KtWeb/rp";
import "libdm/log";
import "cts";
import "db";

/// \{*.} -> s
process = \Rq -> {
  switch (Rq.rq) {
    "getLog": return rp.mkJs({
        Log: log.readJs()
      });
    "resetLog": {
        log.reset();
        return rp.mkEmpty();
      }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};

