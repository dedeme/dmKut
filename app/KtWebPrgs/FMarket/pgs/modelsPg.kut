/// Models page.

import "KtWeb/rp";
import "db";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      sys.print(rp.mkJs({
        modelIds: sys.cmd("fmarket", ["models"])[0],
        mdStats: db.mdStatsJs()
      }));
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};

