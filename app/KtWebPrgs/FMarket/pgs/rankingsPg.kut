/// Rankings page.

import "KtWeb/rp";
import "db";

/// \m -> ()
process = \Rq -> {
  rq = js.rs(Rq.rq);
  switch (rq) {
    "idata": {
      sys.print(rp.mk({
        modelIds: sys.cmd("fmarket", ["models"])[0],
        rankings: db.rankingsJs()
      }));
    }
    default: sys.fail("Value of rq (" + rq + ") is not valid");
  }
};

