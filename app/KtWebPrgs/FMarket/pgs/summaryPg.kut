/// Rankings page.

import "KtWeb/rp";
import "db";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      return rp.mk({
        Summary: db.readSummary()
      });
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};

