/// Accounting annotation entry

/// Create a new entry.
/// \s, s, f -> <accAnn>[s, s, f]
new = \date, description, amount -> return [date, description, amount];;

/// \<accAnn>[s, s, f] -> s
toJs = \AA -> return js.wa([
  js.ws(AA[0]),
  js.ws(AA[1]),
  js.wf2(AA[2], 2)
]);;
