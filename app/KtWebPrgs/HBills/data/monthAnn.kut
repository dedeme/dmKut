/// Monthly annotation entry

/// Create a new entry.
/// \s, [s]|[], f -> <monthAnn>[s, [s]|[], f]
new = \month, description, amount -> return [month, description, amount];;

/// \<monthAnn>[s, [s]|[], f] -> s
toJs = \AA -> return js.wa([
  js.ws(AA[0]),
  arr.empty(AA[1]) ? js.wn() : js.ws(AA[1]!),
  js.wf2(AA[2], 2)
]);;
