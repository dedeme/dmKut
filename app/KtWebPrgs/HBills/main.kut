/// Main script.

import "KtWeb/rp";
import "pgs/bills";
import "pgs/stays";
import "pgs/settings";

Args = sys.args();
ck = Args[0];
Rq = js.ro(Args[1]);

rp.init(ck);

source = js.rs(Rq.source);
switch(source) {
  "Bills": bills.process(Rq);
  "Stays": stays.process(Rq);
  "Settings": settings.process(Rq);
  default: sys.fail("Value of source (" + source + ") is not valid");
}


