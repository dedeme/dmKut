/// Main script.

import "KtWeb/rp";
import "db";
import "pgs/summary";
import "pgs/bills";
import "pgs/stays";

Args = sys.args();
ck = Args[0];
Rq = js.r(Args[1]);

rp.init(ck);
db.init();

switch(Rq.source) {
  "Summary": summary.process(Rq);
  "Bills": bills.process(Rq);
  "Stays": stays.process(Rq);
  default: sys.throw("Value of source (" + Rq.source + ") is not valid");
}


