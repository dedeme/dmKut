/// Stays page.

import "KtWeb/rp";
import "db";
import "data/accAnn";

/// \m -> ()
process = \Rq -> {
  rq = js.rs(Rq.rq);
  switch (rq) {
    "idata": sys.print(rp.mk({
        "anns": js.wa(arr.mp(db.readStays(), \e -> return accAnn.toJs(e);))
      }));
    default: sys.fail("Value of rq (" + rq + ") is not valid");
  }
};

