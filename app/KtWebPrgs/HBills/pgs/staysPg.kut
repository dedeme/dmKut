/// Stays page.

import "KtWeb/rp";
import "db";

/// \{*.} -> s
process = \Rq -> {
  switch (Rq.rq) {
    "idata": return rp.mk({
        Stays: db.readStays()
      });
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};

