/// Stays page.

import "KtWeb/rp";
import "db";
import "data/accAnn";
import "data/monthAnn";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      Years = [];
      for (year = db.YearsRange[0] : db.YearsRange[1] + 1) {
        arr.push(Years, math.itos(year));
      }
      sys.print(rp.mk({
        "years": Years,
        "billAnns": db.readBills(),
        "stayAnns": db.readStays()
      }));
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};

