/// Package index page.

import "KtWeb/rp";
import "readers/ixReader";
import "data/indexTree";

/// Process request.
/// \m -> s
process = \Rq -> {
  switch (Rq.rq) {
    "index": {
      IxT = ixReader.read(Rq.pack);
      sys.println(rp.mk({
        "index": !!IxT
          ? [indexTree.toJs(IxT!)]
          : []
      }));
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};


