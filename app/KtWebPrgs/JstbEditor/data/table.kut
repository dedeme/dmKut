/// File path of a table.

/// Create a new d<table>
/// \s, s -> d<table>
new = \fpath, error -> return { fpath, error };;

/// \<table> -> [*(JS)...]
toJs = \T -> return [T.fpath,  T.error];;

/// \[*(JS)...] -> <table>
fromJs = \A -> return new(A[0], A[1]);;


