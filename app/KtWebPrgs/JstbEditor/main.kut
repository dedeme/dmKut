/// Main script.

import "KtWeb/rp";
import "db";
import "pgs/tlistEditor";
import "pgs/editor";

db.init();

Args = sys.args();
ck = Args[0];
Rq = js.ro(Args[1]);

rp.init(ck);

source = js.rs(Rq.source);
switch(source) {
  "TlistEditor": tlistEditor.process(Rq);
  "Editor": editor.process(Rq);
  default: sys.fail("Value of source (" + source + ") is not valid");
}


