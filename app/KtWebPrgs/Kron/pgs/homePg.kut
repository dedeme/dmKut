/// Home page.

import "libdm/log";
import "KtWeb/rp";
import "db";
import "cts";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      R = sys.cmd2("ps", ["-x"]);
      isActive = str.index(
        R!,
        "kut2 /dm/dmKut/app/KtWebPrgs/Kron/main start"
      ) != -1;
      return rp.mk({isActive});
    }
    "getLog": return rp.mkJs({
        Log: log.readJs()
      });
    "resetLog": {
        log.reset();
        return rp.mkEmpty();
      }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};

