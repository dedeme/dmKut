/// Change pass page.

import "libdm/cgi";

/// Process request.
/// \m -> s
process = \Rq -> {
  rq = js.rs(Rq.rq);
  switch (rq) {
    "changePass": {
      user = js.rs(Rq.user);
      old = js.rs(Rq.old);
      new = js.rs(Rq.new);
      return cgi.changePass(user, old, new);
    }
    default: sys.throw("Value of rq (" + rq + ") is not valid");
  }
};


