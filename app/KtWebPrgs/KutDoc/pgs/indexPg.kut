/// Package index page.

import "KtWeb/rp";
import "readers/ixReader";

/// Process request.
/// \{*.} -> s
process = \Rq -> {
  switch (Rq.rq) {
    "index": {
      return rp.mk({
        treeOp: ixReader.read(Rq.pack)
      });
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};

