/// Module page.

import "KtWeb/rp";
import "readers/mdReader";

/// Process request.
/// \{*.} -> s
process = \Rq -> {
  switch (Rq.rq) {
    "doc": {
      return rp.mk({
        docOp: mdReader.read(Rq.pack, Rq.pth)
      });
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};
