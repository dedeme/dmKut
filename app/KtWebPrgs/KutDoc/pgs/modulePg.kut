/// Module page.

import "KtWeb/rp";
import "readers/mdReader";
import "data/doc";

/// Process request.
/// \m -> s
process = \Rq -> {
  switch (Rq.rq) {
    "doc": {
      Doc = mdReader.read(Rq.pack, Rq.path);
      sys.println(rp.mk({
        "doc": Doc
          ? [doc.toJs(Doc!)]
          : []
      }));
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};


