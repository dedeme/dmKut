/// Pair Model - Int.

/// Constructor
///   model: Model id.
///   value: Math.round (evaluation * 100000).
/// \s, i -> d<ModelFloat>
new = \model, eval ->  return {
  model,
  eval
};;

/// \<ModelEval> -> [*(JS)...]
toJs = \Me -> return [Me.model, Me.eval];;

/// \[*(JS)...] -> <ModelEval>
fromJs = \A -> return new(
    A[0],
    math.ftoi(A[1])
  );;
