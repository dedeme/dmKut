/// Pair Model - Float.

/// \s, f -> d<ModelFloat>
new = \model, value ->  return {
  model,
  value
};;

/// \<ModelValue> -> [*(JS)...]
toJs = \Mf -> return [Mf.model, Mf.value];;

/// \[*(JS)...] -> <ModelValue>
fromJs = \A -> return new(A[0], A[1]);;
