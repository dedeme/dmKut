/// Profits data of all the investors.

/// Creates a profits entry.
///   \s, [f...] -> d<ProfitsEntry>
new = \date, profits -> return {
  date,
  profits
};;

/// \<ProfitsEntry> -> [*(JS)...]
toJs = \Pe -> return [Pe.date, Pe.profits];;

/// \[*(JS)...] -> <ProfitsEntry>
fromJs = \A -> return new(A[0], A[1]);;
