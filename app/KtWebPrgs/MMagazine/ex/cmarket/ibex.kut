/// Ibex historic.

fpath = "/dm/KtWeb/dmcgi/CMarket/ibex.tb";

/// Returns the ibex value for 'date' or [] if it is not found.
/// \i -> [f] | []
read = \date -> {
  d = time.toStr(date);
  Qs = js.r(file.read(fpath))[1];
  for (Q = Qs) {
    if (d == Q[0]) return [Q[2]];
  }
  return [];
};
