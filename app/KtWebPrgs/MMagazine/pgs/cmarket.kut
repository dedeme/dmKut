/// Main module.

import "pgs/cmarket/profitsPg";
import "pgs/cmarket/percentsPg";

/// \m -> s
process = \Rq -> {
  switch (Rq.source) {
    "ProfitsPg": return profitsPg.process(Rq);
    "PercentsPg": return percentsPg.process(Rq);
    default: sys.throw("Value of source (" + Rq.source + ") is not valid");
  }
};
