/// Main module.

import "pgs/mmarket/modelsPg";
import "pgs/mmarket/hotMapsPg";

/// \m -> s
process = \Rq -> {
  switch (Rq.source) {
    "ModelsPg": return modelsPg.process(Rq);
    "HotMapsPg": return hotMapsPg.process(Rq);
    default: sys.throw("Value of source (" + Rq.source + ") is not valid");
  }
};
