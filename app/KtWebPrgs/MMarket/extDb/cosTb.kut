/// Company nicks reader.

import "cts";

/// Returns selected companies sorted by ascendent alphabetic order.
/// \ -> [s.]
read = \ -> {
  :arr Cos = arr.map(
    arr.filter(js.r(file.read(cts.cosPath))[1], \E -> return E[1];),
    \E -> return E[0];
  );
  Cos.sort(\d1, d2 -> return d1 < d2;);
  return Cos;
};
