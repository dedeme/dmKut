/// Quotes reader.

import "libmkt/quotes";
import "libmkt/tquotes";
import "cts";
import "cosTb";

/// Returns quotes of 'Cos'.
/// \[s.] -> <tquotes>
read = \Cos -> {
  return tquotes.read(cts.quotesPath, Cos, true);
};
