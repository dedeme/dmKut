/// Configuration data.

/// Returns configutation data.
///   lang: Applications default language.
/// \s -> d<conf>
new = \lang -> return { lang : lang };;

/// Returns the default configuration data.
mk = \-> return new ("es");;

/// \d<conf> -> [*(JS)...]
toJs = \C -> return [C.lang];;

/// \[*(JS)...] -> d<conf>
fromJs = \A -> return new(A[0]);;
