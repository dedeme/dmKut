/// Companies selected in daily charts.

import "../dailyDb";

fpath = file.cat([dailyDb.dpath, "cosSel.tb"]);

/// Initialize table.
/// \ -> ()
init = \ -> if (!file.exists(fpath)) write([]);;

/// Returns table.
/// \ -> [s.]
read = \ -> return js.r(file.read(fpath));;

/// Write table.
/// \[s.] -> ()
write = \A -> file.write(fpath, js.w(A));;
