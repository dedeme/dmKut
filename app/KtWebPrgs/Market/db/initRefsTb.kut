/// [<initRef>.] table.

import "cts";
import "data/initRef";

fpath = file.cat([cts.dataPath, "initRef.tb"]);

/// Initializes table.
/// \ -> ()
init = \ -> if (!file.exists(fpath)) write([]);;

/// Reads table.
/// \ -> [<initRef>.]
read = \ -> return js.r(file.read(fpath));;

/// Writes table.
/// \[<initRef>.] -> ()
write = \tb -> file.write(fpath, js.w(tb));;
