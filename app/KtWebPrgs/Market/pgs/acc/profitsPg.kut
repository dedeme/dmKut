/// Accounting trading page.

import "KtWeb/rp";
import "cts";
import "data/chart/profitsEntry";
import "db/acc/profitsDb";

/// \{*.} -> s
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      AllEs = []; // [<profitsEntry>.]

      for (y = arr.reverse(profitsDb.investorYears()))
        AllEs.cat(profitsDb.read(y));

      return rp.mk({
        AllEs
      });
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};
