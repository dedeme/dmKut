/// Text data.

/// Constructor
///   text: Message.
/// \s -> s
new = \text -> return text;;

/// Returns an initialized text.
/// \ -> s
init = \ -> return new("");;

/// \s -> s
toJs = \t -> return js.ws(t);;

/// \s -> s
fromJs = \j -> {
  return new(
    js.rs(j)
  );
};
