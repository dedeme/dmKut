/// Main script.

import "KtWeb/rp";
import "db";
import "pgs/home";
import "pgs/settings";

Args = sys.args();
ck = Args[0];
Rq = js.ro(Args[1]);

rp.init(ck);
db.init();

source = js.rs(Rq.source);
switch(source) {
  "Home": home.process(Rq);
  "Settings": settings.process(Rq);
  default: sys.fail("Value of source (" + source + ") is not valid");
}


