/// Main script.

import "KtWeb/rp";
import "db";
import "pgs/home";
import "pgs/settings";

Args = sys.args();
ck = Args[0];
Rq = js.r(Args[1]);

rp.init(ck);
db.init();

switch(Rq.source) {
  "Home": home.process(Rq);
  "Settings": settings.process(Rq);
  default: sys.throw("Value of source (" + Rq.source + ") is not valid");
}


