/// UpdateKutLibs depends on the GNU linux programs 'ln' and 'readlink'.

import "KtWeb/rp";
import "db";
import "pgs/llistEditor";
import "pgs/editor";

db.init();

Args = sys.args();
ck = Args[0];
Rq = js.r(Args[1]);

rp.init(ck);

switch(Rq.source) {
  "LlistEditor": llistEditor.process(Rq);
  "Editor": editor.process(Rq);
  default: sys.throw("Value of source (" + Rq.source + ") is not valid");
}


