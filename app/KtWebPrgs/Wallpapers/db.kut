/// Data base initializer.

import "data/cts";
import "db/sels";
import "db/picts";
import "db/songs";
import "db/danceSongs";

/// \ -> ()
init = \ -> {
	dbpath = cts.dbPath;
	file.mkdir(dbpath);

	sels.init(dbpath);
	picts.init(dbpath);
	songs.init(dbpath);
	danceSongs.init(dbpath);
};

