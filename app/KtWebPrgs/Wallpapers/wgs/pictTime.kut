/// Picture time widget.

import "KtWeb/rp";
import "db/picts";
import "db/sels";

/// \{*.} -> ()
process = \Rq -> {
  rq = Rq.rq;
  switch (rq) {
    "changePictTime": {
      sels.setPictTime(math.ftoi(Rq.value));
      sys.print(rp.mkEmpty());
    }
    default: sys.throw("Value of rq (" + rq + ") is not valid");
  }
};

