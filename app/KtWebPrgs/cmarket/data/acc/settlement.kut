/// Settlement data.

import "ldg";
import "pf";

/// Constructor.
///   ledger   : Settlement ledger.
///   portfolio: Settlement portfolio. It is a dictionary {nick -> [stocks, price]},
///              where price is the unitary average price of each stock.
///   errors   : Errors when calculating ledger and portfolio.
/// \<ledger>, {[i, f]...}, [s...] -> d<settlement>
new = \ledger, portfolio, errors -> return {ledger, portfolio, errors};;
