/// Market day data.

/// Constructor.
///   date  : Day.
///   hopen : Open hour.
///   mopen : Open minute.
///   hclose: Close hour.
///   mclose: Close minute.
/// \s, i, i, i, i -> <marketDay>
new : date, hopen, mopen, hclose, mclose;

/// \marketDay> -> <timetable>
toTimetable = \M -> return M[1:];;

/// \<marketDay> -> JS
toJs = \M -> return M;;

/// \JS -> <marketDay>
fromJs = \A -> return new(
    A[0], math.ftoi(A[1]), math.ftoi(A[2]), math.ftoi(A[3]), math.ftoi(A[4])
  );;
