/// Companies table data.

import "co";

/// Constructor.
///   main: Nick of selected company to verify dates.
///   Cos: Companies.
/// \s, [<co>.] -> d<cosT>
new : main, Cos;

/// \<cosT> -> JS
toJs = \C -> return [C[main], arr.map(C[Cos], co.toJs)];;

/// \JS -> <cosT>
fromJs = \A -> return new(A[0], arr.map(A[1], co.fromJs));;
