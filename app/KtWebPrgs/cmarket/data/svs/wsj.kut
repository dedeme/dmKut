/// Quotes server data MWATCH ("MarketWatch").

import "data/qs/qsv";
import "libmkt/qs";
import "libdm/ext";

// \s -> [s] | []
readVol = \tx -> {
  mkRet = \n -> return [math.itos(math.ftoi(n))];;

  if (tx == "") return ["0"];
  if (str.ends(tx, "K")) {
    vOp = math.fromEn(tx[:-1]);
    if (!!vOp) return mkRet(vOp! * 1000.0);
    return [];
  }
  if (str.ends(tx, "M")) {
    vOp = math.fromEn(tx[:-1]);
    if (!!vOp) return mkRet(vOp! * 1000000.0);
    return [];
  }

  vOp = math.fromEn(tx);
  if (!!vOp) return mkRet(vOp!);
  return [];
};

