/// Companies selected in daily charts.

import "db";

dpath = "daily";

fpath = path.cat([dpath, "cosSel.tb"]);

/// Initialize table.
/// \ -> ()
init = \ -> {
  file.mkdir(db.pathOf(dpath));
  write([]);
};

/// Returns table.
/// \ -> [s...]
read = \ -> return js.r(db.read(fpath));;

/// Write table.
/// \[s...] -> ()
write = \A -> db.write(fpath, js.w(A));;
