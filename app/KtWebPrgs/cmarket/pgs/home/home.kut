/// Home page.

import "KtWeb/rp";
import "libdm/log";
import "data/cts";
import "db";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "getLog":
      sys.print(rp.mkJs({
        log: log.readJs()
      }));
    "resetLog": {
        log.reset();
        sys.print(rp.mkEmpty());
      }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};

