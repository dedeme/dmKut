/// Investor model to send to Javascript.

import "libmkt/model";
import "simParams";

/// Return JSONization of model to send to Javascript, including
/// ParamBases, ParamBaseIncs and ParamEnvIncs.
/// \<model> -> <jsModel>
toJs = \:model md -> return [
    md.id,
    md.name,
    md.doc,
    md.ParamNames,
    md.ParamTypes,
    simParams.paramBases(md.id),
    simParams.paramBaseIncs(md.id),
    simParams.paramEnvIncs(md.id)
  ];;
