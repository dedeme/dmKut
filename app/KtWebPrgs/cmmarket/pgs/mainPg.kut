/// Main page

import "KtWeb/rp";
import "libmkt/model";
import "libmkt/models";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      sys.print(rp.mk({
        modelIds: arr.map(models.list(), \:model md -> return md.id;)
      }));
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};
