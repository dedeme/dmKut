/// Results page

import "KtWeb/rp";
import "libmkt/models";
import "data/modelEval";
import "data/jsModel";
import "db";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      mdId = Rq.modelId;
      sys.print(rp.mk({
        model: jsModel.toJs(models.get(mdId)!),
        evals: arr.map(db.readEvals(mdId).evals, modelEval.toJs)
      }));
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};
