/// Rankings page.

import "KtWeb/rp";
import "db";

/// \m -> ()
process = \Rq -> {
  switch (Rq.rq) {
    "idata": {
      isNear = Rq.isNear;

      sys.print(rp.mk({
        summary: db.getRankings(isNear)
      }));
    }
    default: sys.throw("Value of rq (" + Rq.rq + ") is not valid");
  }
};
