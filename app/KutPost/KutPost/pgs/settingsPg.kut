/// Settings page.

import "db/langTb";

/// \ {*.} -> s
process = \Rq -> {
  switch (Rq.rq) {
    "getLang": return js.w({"lang": langTb.read()});
    "setLang": {
      langTb.write(Rq.lang);
      return "{}";
    }
    default: sys.throw("Value of rq '" + Rq.rq + "' is missing");
  }
};
