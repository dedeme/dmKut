/// Quotes reader.

import "libmkt/quotes";
import "libmkt/tquotes";
import "cts";
import "cosTb";

/// Returns companies selected in cosTb and its <tquotes>.
/// \[s.] -> <tquotes>
read = \(S|F) Cos -> {
  return tquotes.read(cts.quotesPath, Cos, true);
};

