/// Pair Model - Int.

/// Constructor
///   model: Model id.
///   value: Math.round (evaluation * 100000).
/// \s, i -> m<ModelFloat>
new = \model, eval ->  return {
  model,
  eval
};;

/// \<ModelEval> -> s
toJs = \Me -> return js.wa([
  js.ws(Me.model),
  js.wi(Me.eval)
]);;

/// \s -> <ModelEval>
fromJs = \j -> {
  A = js.ra(j);
  return new(
    js.rs(A[0]),
    js.ri(A[1])
  );
};
