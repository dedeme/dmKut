/// Main module.

import "pgs/main/home";

/// \m -> s
process = \Rq -> {
  source = js.rs(Rq.source);
  switch (source) {
    "Home": return home.process(Rq);
    default: sys.fail("Value of source (" + source + ") is not valid");
  };
};
