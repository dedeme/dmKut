/// Main-Dmodule widget.

import "dm/cgi";

/// \m -> s
process = \Rq -> {
  rq = js.rs(Rq.rq);
  switch (rq) {
    "close": {
      sessionId = js.rs(Rq.sessionId);
      cgi.delSession(sessionId);
      return cgi.rpEmpty();
    }
    default: sys.fail("Value of rq (" + rq + ") is not valid");
  };
};
