/// Home page.

import "dm/cgi";
import "dm/log";
import "db";

/// \m -> s
process = \Rq -> {
  rq = js.rs(Rq.rq);
  switch (rq) {
    "idata": {
      return cgi.rp({
        "ok": js.wb(true)
      });
    }
    "getLog": return cgi.rp({
      "log": log.readJs()
      });
    default: sys.fail("Value of rq (" + rq + ") is not valid");
  };
};
