/// <dirs> table.

import "cts";
import "data/dirs";

fpath = file.cat([cts.dataPath, "dirs.tb"]);

/// Initializes table.
/// \ -> ()
init = \ -> if (!file.exists(fpath)) write(dirs.newDefault());;

/// Reads table.
/// \ -> <dirs>
read = \ -> return js.r(file.read(fpath));;

/// Writes table.
/// \<dirs> -> ()
write = \o -> file.write(fpath, js.w(o));;
