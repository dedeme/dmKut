/// Alert message.

import "cts";

/// Show alert message.
/// \s -> ()
show = \msg -> {
  :file fsh = file.cat([cts.dataPath, "cmd.sh"]);
  fsh.write(
    "zenity --warning --text=\"" + msg + "\""
  );
  sys.cmd("bash", [fsh]);
};
