/// Header writer.

import "data/module";
import "data/typedSym";
import "data/type";

/// Returns C header code
/// \<module> -> s
write = \:module md -> {
  Bf = []; //[s.]
  for (:typedSym tsym = md.TopSyms)
    Bf.push("extern Val " + md.id + "_" + tsym.id + ";");
  return Bf.join("\n");
};
