/// Global functions.

/// Returns the message of an exception
/// \s -> s
excMsg = \e -> return e[:str.index(e, "\n  /")];;

/// Returns the message of a 'kexc'
/// \s -> s
kexcMsg = \e -> {
  msg = excMsg(e);
  return msg[6:-7];
};
