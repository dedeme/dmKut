Tm = [0];

fib = \n -> return n <= 1 ? 1 : fib(n - 1) + fib(n - 2);;

Tm! = time.now();
trace "Recursive";
trace fib(30);
trace time.now() - Tm!;

fib2 = \n -> {
  if (n <= 1) return 1;
  N0= [1];
  N1= [1];
  for (i = 2:n+1) {
    tmp = N1!;
    N1! += N0!;
    N0! = tmp;
  }
  return N1!;
};

Tm! = time.now();
trace "For";
trace fib2(30);
trace time.now() - Tm!;

