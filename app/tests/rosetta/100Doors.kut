A = arr.new(100, false);
for (i = 0:100) {
  P = [i];
  inc = i + 1;
  while (P! < 100) {
    p = P!;
    A[p] = !A[p];
    P! += inc;
  }
}
for (p = 0:100)
  sys.println(str.fmt("door #%d is %s.", [p + 1, A[p] ? "open" : "close"]));
