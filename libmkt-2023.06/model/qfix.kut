/// Fix model.

import "model";
import "doc/qfixDoc";

/// Returns a new 'qfix' model.
new = \ -> return model.new(
    "QFIJO",
    "Quantum fijo",
		qfixDoc.text,
		["Intervalo"],
    [model.percParam],
    [0.01],
    [0.005],
    [0.001],
		calc
  );;

// \[[<vec>...]], [f...], (\[<vec>...], [<vec>...] -> ()) -> ()
calc = \Closes, Params, fn -> {
  Env = {Closes, Params};
  for (Row = Closes) fn(Row, sys.c("calcQfix", Env));
};
